  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon",
    "build": "rimraf dist && swc src --out-dir dist --copy-files",
    "start": "node -r dotenv/config dist/src/server.js",
    "lint": "next lint",
    "snaplet:restore": "snaplet snapshot restore --no-reset",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:integration": "pnpm supabase:seed && vitest run --config ./vitest.integration.config.ts",
    "supabase:reset": "npx supabase db reset",
    "supabase:seed": "npx supabase db reset && npx tsx seed.ts",
    "update:sentry:sourcemaps": "npx @sentry/wizard@latest -i sourcemaps",
    "supabase:types": "npx supabase gen types typescript --local > src/types/database-generated.types.ts",
    "supabase:types:data": "npx supabase gen types typescript --project-id zgvoyckkistexkfdmjqc > ./src/types/supabase-data-generated.types.ts"
  },
